<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.paul">
	<author>Paul</author>
	<namespace>jsonservice</namespace>

	<entity name="Data" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="dataId" type="long" primary="true" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />

		<!-- Other fields -->
		<column name="information" type="String" />
		<column name="firstName" type="String" />
		<column name="lastName" type="String" />

		<!-- Order -->
		<order by="asc">
			<order-column name="dataId" />
		</order>
		
	</entity>
	
</service-builder>